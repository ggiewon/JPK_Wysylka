                        <Button Command="{Binding AddNewPurchaseRowCommand}" ContentTemplate="{StaticResource AddPurchaseButtonTemplate}" Style="{StaticResource ToolBarButton}" VerticalAlignment="Top" />
                        <Button Command="{Binding DeletePurchaseRowCommand}" ContentTemplate="{StaticResource DeletePurchaseButtonTemplate}" Style="{StaticResource ToolBarButton}" VerticalAlignment="Top" />
                    </ToolBar>

                  
                </Grid>
            </TabItem>
        </TabControl>
        <StackPanel Grid.Row="1" Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="10">
            <Button ContentTemplate="{StaticResource SaveButtonTemplate}" Command="{Binding SaveCommand}" Margin="5,0,0,0"/>
            <Button ContentTemplate="{StaticResource CancelButtonTemplate}" Command="{Binding CancelCommand}" Margin="5,0,5,0"/>
        </StackPanel>
    </Grid>
    <Window.InputBindings>
        <KeyBinding Key="Escape" Command="{Binding CancelCommand}" />
    </Window.InputBindings>
</Window>
